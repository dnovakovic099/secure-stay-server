  <style>
    @font-face {
      font-family: "Open Sans";
      src: url("/public/fonts/Open_Sans/OpenSans-VariableFont_wdth,wght.ttf") format("truetype");
      font-weight: normal;
      font-style: normal;
      font-display: swap;
    }

    @font-face {
      font-family: "Great Vibes";
      src: url("/public/fonts/Great_Vibes/GreatVibes-Regular.ttf") format("truetype");
      font-weight: normal;
      font-style: normal;
      font-display: swap;
    }

    @font-face {
      font-family: 'Skia';
      src: url('/public/fonts/Skia/Skia.ttf') format('truetype');
      font-weight: normal;
      font-style: normal;
    }

    @font-face {
      font-family: 'Zapfino';
      src: url('/public/fonts/Zapfino/Zapfino.ttf') format('truetype');
      font-weight: normal;
      font-style: normal;
    }

    @font-face {
      font-family: 'Apple Chancery';
      src: url('/public/fonts/Apple_Chancery/Apple-Chancery.ttf') format('truetype');
      font-weight: normal;
      font-style: normal;
    }

    * {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    :root {
      --clr-page-bg: #fefcf5;
      --clr-primary: #d6c27b;
      --clr-primary-dark: rgb(154, 117, 62);
      --font-body: "Open Sans", serif;
      /* --font-cursive: "Playwrite AU SA", serif; */
      --font-cursive: "Zapfino", serif;
      --font-address: "Apple Chancery", serif;
      --font-semi-cursive: "Great Vibes", serif;
      --font-body-text: "Skia", serif;
    }

    html,
    body {
      margin: 0;
      padding: 0;
      width: 100%;
      height: 100%;
      line-height: 1;
    }

    body {
      font-size: 1rem;
      background: linear-gradient(var(--clr-page-bg), var(--clr-page-bg)) padding-box,
        linear-gradient(to right, var(--clr-primary), var(--clr-page-bg)) border-box;
    }

    table {
      width: 100%;
      border-spacing: 0;
      border-collapse: collapse;
      margin: 0;
      padding: 0;
    }

    table tr {
      margin: 0;
      padding: 0;
      page-break-inside: avoid;
    }

    table tr td {
      margin: 0;
      padding: 0;
      vertical-align: top;
    }

    img {
      max-width: 100%;
    }

    .page-1-text h1::after {
      content: "";
      height: 1px;
      width: 70%;
      background: linear-gradient(to bottom, var(--clr-primary), #fff);
      display: block;
      position: relative;
      bottom: 4px;
    }

    .cursive-heading {
      font-family: var(--font-cursive);
      font-weight: 400;
      font-style: normal;
      font-size: 3rem;
      line-height: normal;
      color: var(--clr-primary-dark);
      letter-spacing: -0.25px;
    }

    .page {
      background-color: var(--clr-page-bg);
      position: relative;
      height: 11.6in;
      margin: 0;
      padding: 0;
      overflow: hidden;
      display: flex;
      page-break-before: always;
      page-break-after: always;
      page-break-inside: avoid;
    }

    .page-content {
      position: relative;
      width: 100%;
      width: 100%;
      display: flex;
      flex-direction: column;
      gap: 20px;
      justify-content: space-between;
    }

    .box {
      padding: 18px 25px;
      border-radius: 25px;
      display: flex;
      flex-direction: column;
      gap: 10px;
      background: linear-gradient(var(--clr-page-bg), var(--clr-page-bg)) padding-box,
        linear-gradient(to right, var(--clr-primary), var(--clr-page-bg)) border-box;
      border: 1px solid transparent;
    }

    .box:last-child {
      margin-bottom: 0 !important;
    }

    .box.white {
      background: #fff;
      border-radius: 25px;
      border: 1px solid #e7e7e7;
      box-shadow: 0 4px 4px rgba(0, 0, 0, 0.05);
    }

    .box.tinted {
      background: rgba(157, 145, 107, 0.5);
      color: white;
      font-weight: 700;
    }

    .box.tinted p {
      font-weight: 700;
    }

    .text-black,
    .text-black * {
      color: #000;
    }

    .box-label {
      padding-inline: 8px;
      background: linear-gradient(45deg,
          var(--clr-primary),
          var(--clr-primary-dark));
      width: 100%;
      max-width: 250px;
      color: white;
      font-weight: 500;
      border-radius: 25px;
      height: 35px;
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 18px;
      box-shadow: 0 10px 8px rgba(0, 0, 0, 0.1);
      margin-bottom: 16px;
    }

    p {
      font-weight: 500;
      line-height: normal;
      text-align: justify;
    }

    small {
      font-size: 12px;
    }

    td {
      line-height: 1;
    }

    .table-body-cell,
    .table-header-cell {
      border: 2px solid #fff;
      font-family: var(--font-body);
    }

    .table-body-cell {
      background-color: #fff;
      padding: 8px;
      font-size: 1.2rem;
      font-weight: 600;
    }

    /* .styled-table tbody tr:last-child td {
        border-bottom-right-radius: 18px;
        border-bottom-left-radius: 18px;
      } */
    .styled-table tbody tr:nth-child(even) .table-body-cell {
      background-color: #f9f3e6;
    }

    .styled-table tbody tr:nth-child(odd) .table-body-cell {
      background-color: #f7ecd5;
    }

    .border-image {
      border-radius: 100%;
      border: 14px solid #b79c5f;
    }

    @media print {
      .page {
        margin: 0;
        padding: 0;
        height: 100vh;
        page-break-before: always;
        page-break-after: always;
        page-break-inside: avoid;
      }
      
      table {
        page-break-inside: avoid;
      }
      
      tr {
        page-break-inside: avoid;
      }
      
      td {
        page-break-inside: avoid;
      }
    }

    .portfolio-grid {
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 20px;
      /* padding: 30px 50px; */
    }

    .portfolio-grid img {
      width: 100%;
      height: 185px;
      border-radius: 10px;
      object-fit: cover;
    }

    .padding-inline-50 {
      padding-inline: 50px !important;
    }
  </style>
  <title>
    <%= title %>
  </title>
</head>

